    int findDuplicate(vector<int>& arr) {
        int fast=arr[0];
        int slow=arr[0];
        
        do{
            fast=arr[arr[fast]];
            slow=arr[slow];
        }while(slow!=fast);
        
        fast=arr[0];
        while(fast!=slow){
            fast=arr[fast];
            slow=arr[slow];
        }
        return fast;
    }
